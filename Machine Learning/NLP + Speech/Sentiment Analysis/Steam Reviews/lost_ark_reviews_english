        <script type="text/javascript">
         $J( function() {
		var appid = 1599340;
		var bCheckAppAgeGateBypass = 1;

		CheckAppAgeGateBypass( appid, bCheckAppAgeGateBypass, function( bShowWarning )
		{
			if ( bShowWarning )
			{
				var bResponsivePage = $J( 'html.responsive' ).length;

				var $Content = $J( '<div/>' );
				$Content.append( $Header );
				$Content.append( "<div class=\"adult_content_age_gate\">\r\n\t\t\t<img class=\"adult_content_age_gate_header\" src=\"https:\/\/cdn.akamai.steamstatic.com\/steam\/apps\/1599340\/header.jpg?t=1644892919\">\r\n\t\r\n\t\t\t\t\t\t<p class=\"descriptor\">Content posted in this community<br> may not be appropriate for all ages,<br> or may not be appropriate for viewing at work.<\/p>\r\n\t\t\r\n\t\t<input type=\"checkbox\" id=\"ViewAllForApp\"><label for=\"ViewAllForApp\">Don't warn me again for Lost Ark<\/label>\r\n\t\t<div class=\"adult_content_age_gate_buttons\">\r\n\t\t\t<div class=\"btn_grey_white_innerfade btn_medium\" id=\"age_gate_btn_continue\">\r\n\t\t\t\t<span>View Page<\/span>\r\n\t\t\t<\/div>\r\n\t\t\t&nbsp;\r\n\t\t\t<div class=\"btn_grey_white_innerfade btn_medium\" id=\"age_gate_btn_cancel\">\r\n\t\t\t\t<span>Cancel<\/span>\r\n\t\t\t<\/div>\r\n\t\t<\/div>\r\n\t\r\n\r\n\t\t<div class=\"edit_preferences\">\r\n\t\tYour preferences are configured to warn you when images may be sensitive.<br>\r\n\t\t<a href=\"https:\/\/store.steampowered.com\/account\/preferences#CommunityContentPreferences\">Edit Preferences<\/a>\r\n\t<\/div>\r\n\t\r\n<\/div>\r\n" );

				var fnCleanup = null;

				if ( bResponsivePage )
				{
					var $TemplateContent = $J( '.responsive_page_template_content' ).hide();
					var $Footers = $J( '#footer_spacer, #footer' ).hide();
					$TemplateContent.parent().append( $Content );
					fnCleanup = function ()
					{
						$TemplateContent.show();
						$Footers.show();
					};
				}
				else
				{
					var $Header = $J( '#global_header' );
					$Header.wrap( '<center/>' );
					var $HeaderWrapper = $Header.parent();
					$Header.detach();

					var $VisiblePageElements = $J( 'body > :visible' )
					$VisiblePageElements.hide();

					$J( document.body ).append( $Content );

					fnCleanup = function ()
					{
						$HeaderWrapper.append( $Header );
						$VisiblePageElements.show();
					};
				}

				$Content.find( '#age_gate_btn_cancel' ).click( function ()
				{
					window.history.back();

										window.setTimeout( function ()
					{
						window.location = 'https://steamcommunity.com';
					}, 1000 );
				} );
				$Content.find( '#age_gate_btn_continue' ).click( function ()
				{
					$Content.remove();
					fnCleanup();

					// only for this session
					var strCookie = 'age_gate_' + appid;
					WebStorage.SetLocal( strCookie, true, true );

					// the apphub layout may be broken if it was hidden while this dialog was shown
					// (or if the user resized while this dialog was up) - just trigger a recalc
					$J( window ).trigger( 'resize' );
				} );
				$Content.find( '#ViewAllForApp' ).change( function ()
				{
					var bBypass = $J( "#ViewAllForApp" ).prop( 'checked' );
					SetAppAgeGateBypass( appid, bBypass, function ( data )
					{
						switch ( data.success )
						{
							case 1:
								break;
							default:
								ShowAlertDialog( 'Error', 'Failed to save your preferences' );
						}
					} );
				} );
			}
			else
			{
				g_UGCSkipAdultContentCheckForAppID = appid;
				ReapplyAdultContentPreferences();
			}
		} );
	});
        </script>
