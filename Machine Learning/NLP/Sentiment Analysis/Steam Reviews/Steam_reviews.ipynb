   "source": [
    "#Quick Logistic Ecuation and Representation\n",
    "def yhat(x,b0,b1):\n",
    "    return np.array(np.exp(b0+x*b1) / (1 + np.exp(b0+x*b1)))\n",
    "\n",
    "yhat_sorted = np.sort(yhat(df['Polarity'],results_log.params[0],results_log.params[1]))\n",
    "x_sorted = np.sort(np.array(df['Polarity']))\n",
    "\n",
    "probability_idx = np.where(yhat_sorted >= 0.5)[0][0]\n",
    "neutral_vote_idx = np.where(x_sorted >= 0)[0][0]\n",
    "\n",
    "plt.scatter(df['Polarity'], df['Recommendation_Bool'], alpha= 0.6)\n",
    "plt.scatter(x_sorted[probability_idx], yhat_sorted[probability_idx], c='Red')\n",
    "plt.scatter(x_sorted[neutral_vote_idx], yhat_sorted[neutral_vote_idx], c='Green')\n",
    "plt.xlabel('Polarity', fontsize = 20)\n",
    "plt.ylabel('Recommendation', fontsize = 20)\n",
    "plt.plot(x_sorted,yhat_sorted,color='C10')\n",
    "plt.show()\n",
    "\n",
    "#Result: At -0.15 Polarity there is roughly a 50% chance of being Recommended\n",
    "#Polarity 0 have a 77% chance of being recommended"
   ]
