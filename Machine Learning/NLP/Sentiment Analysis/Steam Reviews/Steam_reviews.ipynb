    "#Confusion Matrix to Compare Yhat (Predicted Values: Polarity_Bool) and Y (Actual Values:Recommendation_Bool) and the accuracy\n",
    "def confusion_matrix_ecuation(predicted_values,actual_values):\n",
    "    \n",
    "        bins=np.array([0,0.5,1])\n",
    "        cm = np.histogram2d(actual_values, predicted_values, bins=bins)[0]\n",
    "        accuracy = (cm[0,0]+cm[1,1])/cm.sum()\n",
    "        return cm, accuracy"
   ]
  },
