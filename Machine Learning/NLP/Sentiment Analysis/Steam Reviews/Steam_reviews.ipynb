   "source": [
    "from bs4 import BeautifulSoup as bs"
    "import requests as r\n",
    "from selenium.webdriver import Chrome\n",
    "from selenium.webdriver.common.by import By\n",
    "from selenium.webdriver.common.keys import Keys\n",
    "import time"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Define URLs\n",
    "url_eng = 'https://steamcommunity.com/app/1599340/reviews/?browsefilter=mostrecent&snr=1_5_100010_&p=1&filterLanguage=english'\n",
    "url_spa = 'https://steamcommunity.com/app/1599340/reviews/?browsefilter=mostrecent&snr=1_5_100010_&p=1&filterLanguage=latam'\n",
    "\n",
    "url_list = [url_eng, url_spa]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "r.get(url_eng).status_code"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Status Code\n",
    "def web_status(url_list):\n",
    "    global url_list_code\n",
    "    url_list_code = [r.get(url).status_code for url in url_list]\n",
    "\n",
    "web_status(url_list)\n",
    "\n",
    "print ('English URL: {} \\nSpanish URL: {}'.format(url_list_code[0],url_list_code[1]))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Create session\n",
    "if url_list_code[0] == 200:\n",
    "    print('status code: 200 - OK\\nopening browser'.title())\n",
    "    driver = Chrome()\n",
    "    driver.get(url_eng)\n",
    "\n",
    "else:\n",
    "    print('error: status code is {}'.title().format(url_list_code[0]))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Inspected results:\n",
    "\n",
    "# <div class=\"btn_grey_white_innerfade btn_medium\" id=\"age_gate_btn_continue\"> ------ To build the Script\n",
    "# <input type=\"checkbox\" id=\"ViewAllForApp\">   -------------------------------------- To build the Script\n",
    "# <div id=\"AppHubCards\" class=\"apphub_Cards\">  -------------------------------------- Review's date and text\n",
    "# <div class='title'> --------------------------------------------------------------- Review's Recommendation"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Script: Select and Click to Continue\n",
    "checkbox_element = driver.find_element(by='id', value=\"ViewAllForApp\")\n",
    "viewpage_button = driver.find_element(by='id', value=\"age_gate_btn_continue\")\n",
    "\n",
    "\n",
    "print ('selecting the checkbox...'.title()), checkbox_element.click(); time.sleep(1)\n",
    "print ('clicking to continue...'.title()), viewpage_button.click(); time.sleep(1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "while True: \n",
    "    #Script: Scroll Down\n",
    "    driver.execute_script(\"window.scrollTo(0, document.body.scrollHeight);\")\n",
    "\n",
    "    #Until Desired Page is Shown in the Source\n",
    "    if driver.page_source.find(\"page10\") != -1:\n",
    "\n",
    "        soup = bs(driver.page_source, 'lxml')\n",
    "        reviews = soup.find_all('div', class_= 'apphub_CardTextContent')\n",
    "        recommendation = soup.find_all('div', class_='title')\n",
    "        print ('{} reviews have been found'.title().format(len(reviews)))\n",
    "        break\n",
    "\n",
    "driver.quit()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Exporting the Soup for Future Tasks and/or just Readability\n",
    "with open('lost_ark_reviews_english', 'wb') as file:\n",
    "    file.write(soup.prettify('utf-8'))"
   ]
